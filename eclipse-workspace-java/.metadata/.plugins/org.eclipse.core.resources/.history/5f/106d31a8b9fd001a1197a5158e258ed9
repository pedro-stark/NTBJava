public class Stack {
	private Object[] data;
	private int stackNr = 0;
	private int maxSize;

	public Stack(int s) {
		this.maxSize = s;
		this.data = new Object[s];
	}

	public void push(Object o) {
		if (stackNr > maxSize) {
			System.out.println("full");
		} else {
			stackNr++;
			data[stackNr] = o;
		}
	}

	public Object pop() {
		Object o = new Object();
		if (stackNr < 0) {
			System.out.println("empty");
		} else {
			o = data[stackNr];
			data[stackNr] = null;
			stackNr--;
		}
		return o;
	}
}
