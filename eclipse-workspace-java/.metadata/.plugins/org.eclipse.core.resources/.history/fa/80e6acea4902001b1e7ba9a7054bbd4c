package KDP.a3_4;

public class Basiswechsel {

	public static void main(String[] args) {
		if (args.length == 0) {
			System.out.println("Sie sollten mindestens eine Zahl als Argument übergeben");
			System.exit(1);
		}
		for (int i = 0; i < args.length; i++) {
		System.out.println(args[i]);
		}
	}
}
